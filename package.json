{
  "name": "software-testing",
  "version": "1.0.0",
  "scripts": {
    "test": "nyc mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "devDependencies": {
    "chai": "^5.1.2",
    "coveralls": "^3.1.1",
    "esm": "^3.2.25",
    "glob": "^11.0.0",
    "mocha": "^10.8.2",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src/*.js"
    ],
    "exclude": [
      "test/*.js"
    ],
    "cache": false,
    "check-coverage": true,
    "extension": [
      ".js"
    ],
    "instrument": true,
    "sourceMap": false
  },
  "dependencies": {
    "software-testing": "file:"
  },
  "type": "module"
}
